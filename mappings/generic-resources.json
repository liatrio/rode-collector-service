{
  "version": "v1alpha4",
  "mappings": {
    "_meta": {
      "type": "rode"
    },
    "properties": {
      "name": {
        "type": "keyword"
      },
      "type": {
        "type": "keyword"
      },
      "created": {
        "type": "date"
      },
      "join": {
        "type": "join",
        "relations": {
          "resource": "version"
        }
      }
    },
    "dynamic_templates": [
      {
        "strings_as_keywords": {
          "match_mapping_type": "string",
          "mapping": {
            "type": "keyword",
            "norms": false
          }
        }
      }
    ]
  }
}
